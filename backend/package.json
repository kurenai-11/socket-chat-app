  "scripts": {
    "dev": "concurrently npm:watch-ts npm:nodemon",
    "start": "node dist/app.js",
    "preview": "npm run build && npm run node",
    "build": "rm -rf dist/ && mkdir dist/ && swc ./src -d dist/ -s false",
