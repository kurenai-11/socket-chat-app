    "pinia": "^2.0.28",
    "reset.css": "^2.0.2",
    "socket.io-client": "^4.5.4",
    "tailwind-merge": "^1.8.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify-json/ion": "^1.1.6",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@unocss/preset-icons": "^0.48.1",
    "@unocss/transformer-variant-group": "^0.48.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "unocss": "^0.48.1",
    "vite": "^4.0.4",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.0.22"
